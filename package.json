{
  "name": "@lblod/ember-rdfa-editor",
  "version": "0.64.0",
  "description": "Ember addon wrapping an RDFa editor with a public API",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/ember-rdfa-editor.git"
  },
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
