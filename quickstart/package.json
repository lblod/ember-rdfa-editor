{
  "name": "quickstart",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for quickstart goes here",
  "repository": "",
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "echo 'don\\'t lint",
    "lintreally": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "vite",
    "test": "vite build --mode development && ember test --path dist",
    "ci": "concurrently 'npm:lint:js' 'npm:lint:types' 'npm:test'",
    "setup": "cd .. && ./quickstart/set-up-quickstart-app.sh && cd quickstart",
    "teardown": "git checkout package.json"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/macros": "^1.19.5",
    "@embroider/core": "^4.3.0",
    "@embroider/vite": "^1.4.3",
    "@embroider/compat": "^4.1.11",
    "@embroider/router": "^3.0.6",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@eslint/js": "^9.39.1",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@glint/template": "^1.7.3",
    "@lblod/ember-rdfa-editor": "workspace:*",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.9.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^3.4.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.9.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^16.5.0",
    "prettier": "^3.7.3",
    "prettier-plugin-ember-template-tag": "^2.1.1",
    "qunit": "^2.24.3",
    "qunit-dom": "^3.5.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^38.0.0",
    "testem": "^3.16.0",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  }
}
