{
  "name": "test-app",
  "private": true,
  "description": "Test app for ember-rdfa-editor addon",
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "start": "vite",
    "test": "concurrently 'pnpm:test:* {@}' --passthrough-arguments --names 'test:' --prefixColors auto",
    "test:ember": "vite build --mode test && ember test --path dist",
    "e2e": "playwright",
    "e2e:open": "playwright test --ui",
    "e2e:open:docker": "./scripts/e2e.sh test --ui",
    "e2e:run": "playwright test --grep-invert @vrt",
    "e2e:run:docker": "./scripts/e2e.sh test",
    "e2e:run:vrt": "./scripts/e2e.sh test --grep @vrt",
    "e2e:run:vrt:update": "./scripts/e2e.sh test --grep @vrt --update-snapshots",
    "e2e:show-report": "playwright show-report"
  },
  "devDependencies": {
    "@appuniversum/ember-appuniversum": "^3.11.0",
